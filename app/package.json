{
  "name": "webdl",
  "productName": "WebDL",
  "version": "11.0.0",
  "description": "A web video downloader",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "Timber1900",
    "email": "timber.gamer@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/renderer/icons/webdl.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "WebDL"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/renderer.tsx",
                  "preload": {
                    "js": "./src/main/preload.ts"
                  },
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@electron-forge/plugin-webpack": "6.0.0-beta.60",
    "@markusantonwolf/tailwind-css-plugin-custom-color-palette": "^1.2.0",
    "@types/chrome": "^0.0.157",
    "@types/express": "^4.17.13",
    "@types/react": "^17.0.32",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "autoprefixer": "^10.3.7",
    "css-loader": "^6.4.0",
    "electron": "22.3.25",
    "esbuild": "^0.12.29",
    "esbuild-loader": "^2.16.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.2",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.4.0",
    "node-loader": "^2.0.0",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^2.2.17",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "execa": "^5.1.1",
    "express": "^4.17.1",
    "ffmpeg-static-electron": "^2.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-simple-timefield": "^3.2.5",
    "youtube-dl-wrap": "^2.1.1",
    "ytdl-core": "^4.9.1",
    "ytpl": "^2.2.3",
    "ytsr": "^3.5.3"
  }
}
