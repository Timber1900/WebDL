{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/chrome": "^0.0.145",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/nw.js": "^0.13.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "express": "^4.17.1",
    "ffmpeg-static": "^4.4.0",
    "nw-react-scripts": "4.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "youtube-dl-wrap": "^2.1.0",
    "ytdl-core": "^4.8.3",
    "ytpl": "^2.2.1"
  },
  "scripts": {
    "start": "nw-react-scripts start --load-extension=./react-devtools",
    "build": "nw-react-scripts build",
    "test": "nw-react-scripts test",
    "eject": "nw-react-scripts eject",
    "css": "npx postcss tailwind.css -o ./src/index.css"
  },
  "eslintConfig": {
    "extends": [
      "nw-react-app",
      "nw-react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "index.html",
  "nwBuilder": {
    "//": "https://github.com/nwjs-community/nw-builder",
    "platforms": [
      "win64"
    ],
    "version": "latest",
    "flavor": "sdk",
    "buildDir": "./build",
    "cacheDir": "./cache",
    "winIco": "./public/favicon.ico"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "autoprefixer": "^10.2.6",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.2.2"
  },
  "window": {
    "icon": "store48.png",
    "title": "Web-dl: A web video downloader",
    "toolbar": true,
    "width": 800,
    "height": 800,
    "min_width": 400,
    "min_height": 400,
    "position": "center"
  }
}
